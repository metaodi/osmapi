<?xml version="1.0" encoding="UTF-8"?>
<osm version="0.6" generator="OpenStreetMap server" copyright="OpenStreetMap and contributors" attribution="http://www.openstreetmap.org/copyright" license="http://opendatacommons.org/licenses/odbl/1-0/">
  <api>
    <version minimum="{{ minVersion }}" maximum="{{ maxVersion }}"/>
    <area maximum="{{ maxArea }}"/>
    <tracepoints per_page="{{ tracepointsPerPage }}"/>
    <waynodes maximum="{{ maxWayNodes }}"/>
    <changesets maximum_elements="{{ maxElements }}"/>
    <timeout seconds="{{ timeout }}"/>
    <status database="online" api="mocked" gpx="online"/>
  </api>
</osm>
